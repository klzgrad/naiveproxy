module ukm.mojom;

import "url/mojo/url.mojom";

struct UkmMetric {
  uint64 metric_hash;
  int64 value;
};

struct UkmEntry {
  int64 source_id;
  uint64 event_hash;
  array<UkmMetric> metrics;
};

interface UkmRecorderInterface {
  AddEntry(UkmEntry entry);
  UpdateSourceURL(int64 source_id, string url);
};
