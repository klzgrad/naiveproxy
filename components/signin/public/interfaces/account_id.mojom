module signin.mojom;

// Matches AccountType of AccountId.
enum AccountType {
  UNKNOWN,
  GOOGLE,
  ACTIVE_DIRECTORY,
};

// Mirror of AccountId in Mojo.
struct AccountId {
  AccountType account_type;
  string id;
  string user_email;
};
