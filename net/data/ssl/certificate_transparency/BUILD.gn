action_foreach("ct_log_list") {
  script = "//net/tools/ct_log_list/make_ct_known_logs_list.py"
  sources = [
    "log_list.json",
  ]
  outputs = [
    "${target_gen_dir}/{{source_name_part}}-inc.cc",
  ]
  args = [
    "{{source}}",
    rebase_path("${target_gen_dir}/{{source_name_part}}-inc.cc",
                root_build_dir),
  ]
}
