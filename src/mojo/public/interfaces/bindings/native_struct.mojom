[JavaPackage="org.chromium.mojo.native_types"]
module mojo.native;

enum SerializedHandleType {
  MOJO_HANDLE,
  PLATFORM_FILE,
  WIN_HANDLE,
  MACH_PORT,
  FUCHSIA_HANDLE,
};

struct SerializedHandle {
  handle the_handle;
  SerializedHandleType type;
};

[CustomSerializer]
struct NativeStruct {
  array<uint8> data;
  array<SerializedHandle>? handles;
};
