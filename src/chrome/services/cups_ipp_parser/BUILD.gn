import("//build/config/features.gni")
import("//services/service_manager/public/service_manifest.gni")

source_set("lib") {
  sources = [
    "cups_ipp_parser_service.cc",
    "cups_ipp_parser_service.h",
    "ipp_parser.cc",
    "ipp_parser.h",
  ]

  deps = [
    "//base",
    "//mojo/public/cpp/bindings",
