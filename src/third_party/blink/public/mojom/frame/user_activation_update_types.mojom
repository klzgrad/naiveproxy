module blink.mojom;

// Types of UserActivationV2 state updates sent between the browser and the
// renderer processes.
enum UserActivationUpdateType {
  kNotifyActivation,
  kNotifyActivationPendingBrowserVerification,
  kConsumeTransientActivation,
  kClearActivation
};
