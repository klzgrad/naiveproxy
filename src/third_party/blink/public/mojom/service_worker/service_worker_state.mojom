module blink.mojom;

// Represents ServiceWorkerState from the Service Workers spec:
// https://w3c.github.io/ServiceWorker/#enumdef-serviceworkerstate
enum ServiceWorkerState {
  kParsed,
  kInstalling,
  kInstalled,
  kActivating,
  kActivated,
  kRedundant,
};
