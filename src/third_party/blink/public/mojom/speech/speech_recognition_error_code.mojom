module blink.mojom;

// Used by the browser's speech recognition engine to indicate the reason for a
// speech recognition error.
enum SpeechRecognitionErrorCode {
  kNone,
  kNoSpeech,
  kAborted,
  kAudioCapture,
  kNetwork,
  kNotAllowed,
  kServiceNotAllowed,
  kBadGrammar,
  kLanguageNotSupported,
  kNoMatch
};
