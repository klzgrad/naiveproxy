module blink.mojom;

import "components/schema_org/common/metadata.mojom";
import "url/mojom/url.mojom";

struct WebPage {
  url.mojom.Url url;
  string title;
  array<schema_org.mojom.Entity> entities;
};

// Null page denotes no results.
interface DocumentMetadata {
  GetEntities() => (WebPage? page);
};
