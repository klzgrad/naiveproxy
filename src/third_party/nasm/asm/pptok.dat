#
# A * at the end indicates a condition; the list of conditions are
# on lines starting with *; the negatives are auto-generated
#

# Condition stems. %if MUST BE FIRST in this list.
%if*
%elif*

# Condition tests
*
*ctx
*def
*defalias
*empty
*env
*id
*idn
*idni
*macro
*num
*str
*token
*usable
*using

# Directives with -i- versions for case insensitive
%!assign
%!defalias
%!define
%!defstr
%!deftok
%!macro
%!pathsearch
%!rmacro
%!strcat
%!strlen
%!substr
%!xdefine
%un!macro

# Other directives
%aliases
%arg
%clear
%depend
%else
%endif
%endm
%endmacro
%endrep
%error
%exitmacro
%exitrep
%fatal
%include
%line
%local
%null
%pop
%pragma
%push
%rep
%repl
%rotate
%stacksize
%undef
%undefalias
%use
%warning
