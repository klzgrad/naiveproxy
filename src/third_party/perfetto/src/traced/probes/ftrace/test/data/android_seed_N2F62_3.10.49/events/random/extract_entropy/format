name: extract_entropy
ID: 383
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:const char * pool_name;	offset:8;	size:4;	signed:0;
	field:int nbytes;	offset:12;	size:4;	signed:1;
	field:int entropy_count;	offset:16;	size:4;	signed:1;
	field:unsigned long IP;	offset:20;	size:4;	signed:0;

print fmt: "%s pool: nbytes %d entropy_count %d caller %pF", REC->pool_name, REC->nbytes, REC->entropy_count, (void *)REC->IP
