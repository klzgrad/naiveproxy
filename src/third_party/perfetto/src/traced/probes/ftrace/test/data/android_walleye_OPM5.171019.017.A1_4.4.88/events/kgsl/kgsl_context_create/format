format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:__data_loc char[] device_name;	offset:8;	size:4;	signed:0;
	field:unsigned int id;	offset:12;	size:4;	signed:0;
	field:unsigned int flags;	offset:16;	size:4;	signed:0;
	field:unsigned int priority;	offset:20;	size:4;	signed:0;
	field:unsigned int type;	offset:24;	size:4;	signed:0;

print fmt: "d_name=%s ctx=%u flags=%s priority=%u type=%s", __get_str(device_name), REC->id, REC->flags ? __print_flags(REC->flags, "|", { 0x00000002 , "NO_GMEM_ALLOC" }, { 0x00000010, "PREAMBLE" }, { 0x00000020, "TRASH_STATE" }, { 0x00000008, "CTX_SWITCH" }, { 0x00000040, "PER_CONTEXT_TS" }, { 0x00000080, "USER_TS" }, { 0x00000200, "NO_FT" }, { 0x10000000, "INVALIDATE_ON_FAULT" }, { 0x00000800, "PWR" }, { 0x00000001, "SAVE_GMEM" }) : "None", REC->priority, __print_symbolic(REC->type, { 0, "ANY" }, { 1, "GL" }, { 2, "CL" }, { 3, "C2D" }, { 4, "RS" })
