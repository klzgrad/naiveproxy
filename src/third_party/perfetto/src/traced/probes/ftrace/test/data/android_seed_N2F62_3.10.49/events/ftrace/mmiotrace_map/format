name: mmiotrace_map
ID: 8
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:resource_size_t phys;	offset:8;	size:4;	signed:0;
	field:unsigned long virt;	offset:12;	size:4;	signed:0;
	field:unsigned long len;	offset:16;	size:4;	signed:0;
	field:int map_id;	offset:20;	size:4;	signed:1;
	field:unsigned char opcode;	offset:24;	size:1;	signed:0;

print fmt: "%lx %lx %lx %d %x", (unsigned long)REC->phys, REC->virt, REC->len, REC->map_id, REC->opcode
