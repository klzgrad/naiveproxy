static_library("net_log") {
  sources = [
    "chrome_net_log.cc",
    "chrome_net_log.h",
    "net_export_file_writer.cc",
    "net_export_file_writer.h",
    "net_export_ui_constants.cc",
    "net_export_ui_constants.h",
  ]

  deps = [
    "//base",
    "//components/version_info",
    "//net",
    "//services/network/public/mojom",
