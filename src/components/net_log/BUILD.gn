    "net_export_file_writer_unittest.cc",
  ]
  deps = [
    ":net_log",
    "//base",
    "//base/test:test_support",
    "//net",
    "//net:test_support",
    "//services/network:network_service",
    "//services/network:test_support",
    "//services/network/public/cpp",
    "//services/network/public/cpp:cpp_base",
    "//testing/gtest",
  ]
}
