import("//components/nacl/features.gni")

if (enable_nacl && (is_linux || is_win)) {
  executable("mock_nacl_gdb") {
    sources = [
      "mock_nacl_gdb.cc",
    ]
    deps = [
      "//base",
      "//build/config:exe_and_shlib_deps",

      # Default manifest on Windows (a no-op elsewhere).
      "//build/win:default_exe_manifest",
    ]
  }
}
