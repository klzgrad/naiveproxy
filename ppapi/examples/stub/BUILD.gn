import("//ppapi/examples/ppapi_example.gni")

group("stub") {
  testonly = true
  deps = [
    ":c_stub",
    ":cc_stub",
  ]
}

ppapi_example("c_stub") {
  output_name = "ppapi_example_c_stub"
  sources = [
    "stub.c",
  ]
  deps = [
    "//ppapi/c",
  ]
}

ppapi_example("cc_stub") {
  output_name = "ppapi_example_cc_stub"
  sources = [
    "stub.cc",
  ]
  deps = [
    "//ppapi/cpp",
  ]
}
